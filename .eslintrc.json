{   
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "parser": "typescript-eslint-parser",
    "installedESLint": true,
    "plugins": [
        "import"
    ],
    "rules": {
        "semi": "error",
        "no-unused-vars": "warn",
        "array-bracket-spacing": "warn",
        "arrow-spacing": "warn",
        "block-spacing": "warn",
        "comma-dangle": "error",
        "comma-style": "warn",
        "quotes": ["error", "single"]
    }
}